{
    "release": {
        "branches": [
            "master",
            "next"
        ]
    },
    "plugins": [
        [
            "@semantic-release/commit-analyzer",
            {
                "preset": "angular",
                "releaseRules": [
                    {
                        "type": "docs",
                        "scope": "README",
                        "release": "patch"
                    },
                    {
                        "type": "refactor",
                        "release": "patch"
                    },
                    {
                        "scope": "no-release",
                        "release": false
                    }
                ]
            }
        ],
        "@semantic-release/release-notes-generator"
    ]
}